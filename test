package com.usbank.stickyheaderrecyclerview

 data class TransactionDetails(val merchantName:String = "", val transactionDate:String = "", val  merchantCategory:String = "", val isCheck:Boolean = false, val displayImage:Boolean = false,
                               val debitCreditType:String = "", val returnableIndicator:Boolean = false, val debitSumAmount:String = "", val merchantSubCategoryShortDescription:String = "",
                               val transactionId:String = "", val debitIndicator:Boolean = false, val pendingTransaction:Boolean = false, val transactionAmount:String = "", val transactionDescription:String = "",
                               val merchantSubCategory:String = "", val effectiveDate:String = "", val merchantCategoryShortDescription:String = "", val isDeposit:Boolean = false, val postDate:String = "",
                               val intradayLedgerBalance:String = "", val transactionCategory:String = "", val creditIndicator:String = "", var viewType:Int = -1, var transactionsCount:Int = 0)

data class SuccessMessageBean(val accountNumber:String, val currentPostedBalance:String, val accountNickName:String, val  transactionDetailsViewBeanList:List<TransactionDetails>,
                              val availableBalance:String, val pendingCredit:String, val accountOpenDate:String, val totalAvailableBalance:String, val salesName:String, val transactionsAvailableStatus:String,
                              val accountType:String,val accountStatusCode:String, val totalNoOfPages:Int,val totalTransactionsCount:Int, val accountName:String, val accountId:String, val pendingDebit:String)


data class TransactionsResponse(val viewName:String, val  successMessageBean:SuccessMessageBean)

sealed class TransactionType(val type:Int){
    object PENDING_HEADER : TransactionType(0)
    object PENDING_ITEM : TransactionType(1)
    object POSTED_HEADER : TransactionType(2)
    object POSTED_ITEM : TransactionType(3)
}
